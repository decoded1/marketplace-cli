# Marketplace CLI API Keys Configuration
# Copy this file to .env and fill in your actual API keys
# NEVER commit the .env file to version control

# ============================================
# Apify API Keys
# ============================================
# Get your API key from: https://console.apify.com/account/integrations
APIFY_API_KEY=your_apify_api_key_here

# ============================================
# eBay API Keys
# ============================================
# Get your API credentials from: https://developer.ebay.com/
EBAY_APP_ID=your_ebay_app_id_here
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_AUTH_TOKEN=your_ebay_auth_token_here

# ============================================
# Etsy API Keys
# ============================================
# Get your API key from: https://www.etsy.com/developers/documentation
ETSY_API_KEY=your_etsy_api_key_here
ETSY_SHARED_SECRET=your_etsy_shared_secret_here
ETSY_OAUTH_TOKEN=your_etsy_oauth_token_here
ETSY_OAUTH_TOKEN_SECRET=your_etsy_oauth_token_secret_here

# ============================================
# Scraping Service API Keys
# ============================================

# ZenRows - https://www.zenrows.com/
ZENROWS_API_KEY=your_zenrows_api_key_here

# Crawlbase - https://crawlbase.com/
CRAWLBASE_API_KEY=your_crawlbase_api_key_here

# ScraperAPI - https://www.scraperapi.com/
SCRAPERAPI_KEY=your_scraperapi_key_here

# ScrapingAnt - https://scrapingant.com/
SCRAPINGANT_API_KEY=your_scrapingant_api_key_here

# Bright Data - https://brightdata.com/
BRIGHTDATA_API_KEY=your_brightdata_api_key_here
BRIGHTDATA_ZONE=your_brightdata_zone_here
BRIGHTDATA_USERNAME=your_brightdata_username_here
BRIGHTDATA_PASSWORD=your_brightdata_password_here

# ============================================
# Facebook API Keys (if using official API)
# ============================================
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token_here

# ============================================
# Email Notifications (for tools like SearchExpander)
# ============================================
EMAIL_FROM=your_email@example.com
EMAIL_TO=recipient_email@example.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_smtp_username_here
SMTP_PASSWORD=your_smtp_password_here

# ============================================
# Database Configuration (optional)
# ============================================
DATABASE_URL=your_database_url_here
MONGODB_URI=your_mongodb_uri_here

# ============================================
# General Configuration
# ============================================
# Set to 'production' or 'development'
NODE_ENV=development

# Rate limiting settings (requests per minute)
RATE_LIMIT=60

# Default output format (json, csv, html)
OUTPUT_FORMAT=json

# Proxy settings (if needed)
PROXY_URL=your_proxy_url_here
PROXY_USERNAME=your_proxy_username_here
PROXY_PASSWORD=your_proxy_password_here
